<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".kanatyu_activity">


    <Button
        android:id="@+id/Kana_home_button"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="12dp"
        android:layout_marginBottom="2dp"
        android:layout_weight="1"
        android:text="ホーム"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/Kana_School_button"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/Kana_School_button"
        android:layout_width="110dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="2dp"
        android:layout_weight="1"
        android:text="スクールバス"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/Kana_Kanachu_button"
        app:layout_constraintStart_toEndOf="@+id/Kana_home_button" />

    <Button
        android:id="@+id/Kana_Kanachu_button"
        android:layout_width="110dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="3dp"
        android:layout_marginBottom="2dp"
        android:layout_weight="1"
        android:text="神奈中バス"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/Kana_mypage_button"
        app:layout_constraintStart_toEndOf="@+id/Kana_School_button" />

    <Button
        android:id="@+id/Kana_mypage_button"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="2dp"
        android:layout_weight="1"
        android:text="マイページ"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/Kana_Kanachu_button" />


    <WebView
        android:id="@+id/webView2"
        android:layout_width="290dp"
        android:layout_height="581dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="48dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>